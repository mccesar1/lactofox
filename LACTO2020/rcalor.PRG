SET TALK OFF
SET PATH TO C:\LACTO2006\LX43
USE REG ORDER 2 IN 1
USE CALOR ORDER 2 IN 2

SELECT REG
SET RELATION TO ID INTO CALOR

SCAN 
XID=REG.ID

	SELECT CALOR
	SEEK REG.ID
	IF SEEK(REG.ID)
	SCAN WHILE CALOR.ID=REG.ID
	IF CALOR.STAT="INSEM"
		REPLACE REG.IDT WITH CALOR.TORO
		REPLACE REG.UTEC WITH CALOR.TEC
	ENDIF
	
	IF CALOR.STAT="TRATA"
		REPLACE REG.FPRO WITH CALOR.FECHA
		REPLACE REG.MEDP WITH CALOR.OBSC
	ENDIF
	
	ENDSCAN
	ENDIF
	SELECT REG
ENDSCAN	
BROWSE FIELDS ID,NS,IDT,UTEC,STAT,FPRO,MEDP
CLOSE DATABASES 
