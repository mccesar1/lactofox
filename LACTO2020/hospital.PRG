CLEAR
CLEAR ALL
SET TALK ON
SET CENTURY ON
SET DATE ANSI
SET CLOCK OFF
SET SAFETY OFF
SET DELETED ON

USE C:\LACTO2006\LX18\CLIN ORDER 2

PUBLIC xENF
SELECT CLIN
SET FILTER TO ENF>0

XID=CLIN.ID
xENF=0

SCAN
	SEEK XID
		SCAN WHILE XID=CLIN.ID
			REPLACE CLIN.EVENTO WITH IIF(CLIN.ENF=XENF,'','I')
			IF TRAT=ALLTRIM("ALTA")
				REPLACE EVENTO WITH 'S'
			ENDIF	
			xENF=CLIN.ENF
		ENDSCAN
		XID=CLIN.ID
ENDSCAN

BROWSE


