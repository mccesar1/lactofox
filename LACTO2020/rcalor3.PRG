SET TALK OFF
CLOSE DATABASES

USE C:\COW2000\COWMAE01 ALIAS COW IN 1 EXCLUSIVE
USE C:\LACTO2006\LX43\AGENDA IN 2 EXCLUSIVE

SELECT AGENDA
ZAP

SELECT COW
SCAN 
	IF COW.FEC_PROSTA>=DATE()-6
	SELECT AGENDA
	APPEND BLANK
		REPLACE AGENDA.ID WITH COW.NOVAC
		REPLACE AGENDA.NP WITH VAL(COW.NOLAC)
		REPLACE AGENDA.FECHA WITH COW.FEC_PROSTA
		REPLACE AGENDA.AGN WITH 1
		REPLACE AGENDA.TIPO WITH "R"
		REPLACE AGENDA.DX WITH 'REPRODUCCION'
		REPLACE AGENDA.TX WITH 'PROSTAGLANDINA'
		REPLACE AGENDA.M1 WITH 'PG1'
		REPLACE AGENDA.QIA WITH 'S'
				
	SELECT COW
	ENDIF	
ENDSCAN

SELECT AGENDA
BROWSE FIELDS ID,NP,FECHA,AGN,TIPO,DX,TX,M1,QIA
CLOSE DATABASES 
