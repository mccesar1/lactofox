CLEAR
CLEAR ALL

Use IFASE IN 0
SELECT IFASE

SET TALK OFF
SET SYSMENU AUTOMATIC
SET CLOCK OFF
SET EXCLUSIVE OFF
SET STATUS BAR OFF
SET SAFETY OFF
SET PATH TO
SET DELETED ON
SET CPDIALOG OFF
SET SECONDS ON
SET HELP ON
*ON ERROR DO CTRLERROR With Error(),Message(),Lineno(),Program()
SET HELP TO LACTOHLP.CHM
SET HOURS TO 24
SET SYSFORMATS OFF
SET CENTURY ON

* Formato de Fecha
* ----------------
DO CASE
CASE  IFASE.FECHA=1
	SET DATE TO ANSI
CASE IFASE.FECHA=2
	SET DATE TO GERMAN
ENDCASE

* MultiUsuario
* ------------
SET REFRESH TO 2,2
SET REPROCESS To 5,2

PUBLIC B,HOY,xCAPTION,xxNOMBRE,NID,XID,TIT,NOM,MFPES,CLAVEX,_SERIE,SMEDIC,SIGN,cmd_nam0,;
	   cmd_nam5,x305,WLI,PAL,QPAC,QREP,XREG,T2,xmed1,xAREA1,xAREA2,xBASE,xRUTA,XNUM,;
	   xLLAVE1,xLLAVE2,xLLAVE3,xLLAVE4,xLLAVE5,xINICIO,XX,XAYUDA,xFECHA,xCHOICE,xTIPO,xDISK,xDISCO

PUBLIC Q1,Q2,Q3,Q4,Q5,Q6,Q7,Q8,Q9,Q10,Q11,Q12,Q13,Q14,Q15,Q16,Q17,Q18,Q19,Q20,Q21,Q22,Q23,Q24,;
	   Q25,Q26,Q27,Q28,Q29,Q30,Q31,Q32,Q33,Q34,Q35,Q36,Q37,Q38,Q39,Q40,Q41,Q42,Q43,Q44,Q45,Q45,;
	   Q46,QVERSION,TVERSION,xFIN,xUSB,xxNOMBRE,xFONT,xSERIE,xANCHO,xALTO,NTIT2,NTIT3,NTIT4,vTOTAL,DABT,;
	   TOTAL1,TOTAL2,xFPAR,xCOLOR,xCOLOR2,xCOLOR3,xDSC,xCOLORG,xCOLOR4,xCOLOR5

DECLARE SetLicenseKey IN "Agroplus01.dll" STRING RegCode
DECLARE STRING GetSerialNumber IN "Agroplus01.dll" INTEGER DriveIndex
DECLARE STRING GetDiskVolumeNumber IN "Agroplus01.dll" STRING DriveLetter

RegCode='N35G2-SUR8C-F7KLH-34SSK-NDJR5'
SetLicenseKey(RegCode)


DIMENSION TIT(20)
TIT(1)="DAIRYFOX - Aviso"
TIT(2)="ID No Valida  !!    "
TIT(3)="ID Ya Existe  !!    "
TIT(4)="Fecha No Valida  !!   "
TIT(5)="Dato No Valido  !!    "
TIT(6)="Faltan Datos Requeridos !!    "

*xRUTA="C:\"
xRUTA=""
B=CTOD("\")
xAREA1="VIENTRES"
xAREA2="CRIANZA"
Q36='95001001'
xCHOICE=0
xxNOMBRE=""
HOY=Date()
xVERSION=STR(IFASE.Nversion,4,5)
xANCHO=21
xALTO=22

xCOLOR=ICASE(IFASE.COLOR1=1,'RGB(0,0,45)',IFASE.COLOR1=2,'RGB(30,30,45)',IFASE.COLOR1=3,'RGB(170,170,170)')
xCOLOR2=IFASE.COLOR2
xCOLOR3=IFASE.COLOR3
xCOLOR4=IIF(IFASE.COLOR4=1,RGB(0,88,176),RGB(255,0,255))

xCOLORG='RGB(0,88,176)'

WITH _SCREEN
	.Caption="DAIRYFOX Software Lechero."
	.Windowstate=2
	.Icon="LOGO11.Ico"
	.Backcolor=&xCOLOR  
	.LockScreen=.F. 
	.Closable=.F.
	.Movable=.F.
	.MinButton=.T.
	.MaxButton=.T.
	
	
	* INCLUIR EL LOGOTIPO AGROPLUS
	* ----------------------------
*	@0,0 SAY 'LOGODF.JPG' BITMAP CENTER

	WITH _SCREEN
		.AddObject("OlMg","Image")
		.OlMg.Picture="LOGO32.PNG"
		.Olmg.TOP=(_Screen.Height-_screen.olmg.Height)/2
		.Olmg.Left=(_Screen.Width-_screen.olmg.Width)/2
		.Olmg.Anchor=240
		.Olmg.Stretch=1
		.Olmg.Visible=.T.
	ENDWITH
	* ----------------------------

	QVersion="  DAIRYFOX Version "+Str(IFASE.nVersion,4,2)+'.29.7' 
	
	TVersion=[DAIRYFOX v. ]+Str(IFASE.nVersion,4,2)
	xCaption=QVersion+"        "+DTOC(Date())+"    " 
	
	xLLAVE5=Str(IFASE.nVersion,4,2)
	
	* Ayuda en Tarjetas
	* ----------------
	XAYUDA=IFASE.MAYUDA
	xFONT=IFASE.CPP
	USE

xINICIO=SYS(5)+CURDIR()
xRUTA=SYS(5)+CURDIR()

If Not FILE("LDATOS.DBF")
	COPY FILE LDATOS.DAT TO LDATOS.DBF
ENDIF

*USE LDATOS IN 1
*SELECT LDATOS
*xCAPTION=xCAPTION+"  "+LDATOS.LNOMBRE
*.Caption=xCAPTION

ENDWITH

DO FORM CLAVES

READ EVENTS
RETURN



